#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules/.pnpm/jiti@1.21.0/node_modules/jiti/bin/node_modules:/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules/.pnpm/jiti@1.21.0/node_modules/jiti/node_modules:/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules/.pnpm/jiti@1.21.0/node_modules:/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules/.pnpm/node_modules:/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules:/mnt/c/Users/PC/Desktop/Jade/node_modules:/mnt/c/Users/PC/Desktop/node_modules:/mnt/c/Users/PC/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/PC/Desktop/Jade/february-declaration/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules/.pnpm/jiti@1.21.0/node_modules/jiti/bin/node_modules:/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules/.pnpm/jiti@1.21.0/node_modules/jiti/node_modules:/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules/.pnpm/jiti@1.21.0/node_modules:/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules/.pnpm/node_modules:/mnt/c/Users/PC/Desktop/Jade/February-Declaration/node_modules:/mnt/c/Users/PC/Desktop/Jade/node_modules:/mnt/c/Users/PC/Desktop/node_modules:/mnt/c/Users/PC/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/PC/Desktop/Jade/february-declaration/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jiti.js" "$@"
else
  exec node  "$basedir/../../bin/jiti.js" "$@"
fi
